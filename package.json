{
  "name": "extreme-planning-poker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "elm": "elm make ./elm/Main.elm --output=./public/js/main.js",
    "dev": "concurrently --kill-others \"npm run node-live\" \"npm run elm-live\"",
    "node-live": "nodemon ./bin/www",
    "elm-live": "elm-live ./elm/Main.elm --dir=./public/js --no-server -- --output=./public/js/main.js --debug"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "elm-live": "^4.0.2",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.6",
    "pug": "2.0.0-beta11",
    "serve-favicon": "^2.5.0",
    "sha1": "^1.1.1",
    "sse": "0.0.8",
    "uuid": "^8.3.1",
    "ws": "^7.4.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
