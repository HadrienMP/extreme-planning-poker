{
  "name": "extreme-planning-poker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run prod:build-back && npm run prod:build-front",
    "start": "npm run prod:server",
    "prod:build-front": "./elm-make.sh",
    "prod:build-back": "tsc",
    "prod:server": "node ./dist/app.js",
    "dev:build-front": "elm-live ./elm/Main.elm --dir=./public/js --no-server -- --output=./public/js/main.js --debug",
    "dev:build-back": "tsc --watch --preserveWatchOutput",
    "dev:server": "nodemon ./dist/app.js",
    "dev:all": "concurrently --kill-others \"npm run dev:build-front\" \"npm run dev:build-back\" \"npm run dev:server\""
  },
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "@types/serve-favicon": "^2.5.1",
    "@types/sha1": "^1.1.2",
    "debug": "~2.6.9",
    "elm-live": "^4.0.2",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "immutable": "^4.0.0-rc.12",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.6",
    "pug": "2.0.0-beta11",
    "serve-favicon": "^2.5.0",
    "sha1": "^1.1.1",
    "sse": "0.0.8",
    "ts-md5": "^1.2.7",
    "typescript-monads": "^5.0.0",
    "uuid": "^8.3.1",
    "ws": "^7.4.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "typescript": "^4.1.2"
  }
}
